{
    "version": 2,
    "framework": null,
    "functions": {
        "api/*.py": {
            "runtime": "python3.9"
        }
    },
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python@3.1.30"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "main.py"
        }
    ],
    "env": {
        "PYTHONPATH": ".",
        "PYTHON_VERSION": "3.9"
    }
}