{
    "version": 2,
    "functions": {
        "api/*.py": {
            "runtime": "@vercel/python@3.1.30",
            "memory": 3008
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.py"
        }
    ],
    "build": {
        "env": {
            "PIP_NO_BINARY": ""
        }
    }
}